Int thread1count = 0;
Int thread2count = 0;

Void
thread2() {
	Int i = 100;
	while(i > 95) {
		thread2count = thread2count + 1;
		i = i - 1;
		yield;
	}
}

Void
thread1() {
	Int i = 3;
	while(i > 0) {
		thread1count = thread1count + 1;
		i = i - 1;
		spawn thread2;
		yield;
	}
}

Int
main() {
	spawn thread1;
	return 0;
}
