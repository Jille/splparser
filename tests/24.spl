Void
thread1() {
	Int start = 5;
	Int stop = 10;
	Int i = start;
	while(i <= stop) {
		putint(i);
		putchar(10);
		i = i + 1;
		if(i % 2 == 0) {
			yield;
		}
	}
}

Void
thread2() {
	Int start = 20;
	Int stop = 30;
	Int i = start;
	while(i <= stop) {
		putint(i);
		putchar(10);
		i = i + 1;
		if(i % 3 == 0) {
			yield;
		}
	}
}

Int
main() {
	spawn thread1;
	spawn thread2;
	spawn thread1;
	return 1;
}
